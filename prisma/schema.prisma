generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model admin {
  id_admin       Int     @id
  nama_admin     String? @db.VarChar(225)
  username_admin String? @db.VarChar(225)
  password_admin String? @db.VarChar(20)
  role           String? @db.VarChar(225)
}

model barang {
  id_barang    Int     @id @default(autoincrement())
  nama_barang  String? @db.VarChar(255)
  harga_barang Int?
  stok_barang  Int?
}

model kasir {
  id_kasir       Int     @id
  nama_kasir     String? @db.VarChar(225)
  username_kasir String? @db.VarChar(225)
  password_kasir String? @db.VarChar(20)
  role           String? @db.VarChar(225)
}

model transaksi {
  id_transaksi Int        @id(map: "transaks_pkey")
  tanggal      DateTime[] @db.Timestamp
  id_kasir     Int?
}

model users {
  id_users Int     @id @default(autoincrement())
  nama     String? @db.VarChar(225)
  username String? @db.VarChar(225)
  password String?
  role     String? @db.VarChar(225)
}
